{
	"name": "proyect_base_node",
	"version": "1.1.0",
	"description": "proyect init node ",
	"main": "index.js",
	"scripts": {
		"build:ts": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
		"build:assets": "copyfiles -u 1 \"src/**/*.{hbs,yml,json}\" dist/",
		"build": "npm run build:ts && npm run build:assets",
		"start": "nodemon dist/app.js",
		"watch": "nodemon --watch \"src/*\" --ext \"ts,json\" --exec \"ts-node -r tsconfig-paths/register src/app.ts\""
	},
	"author": "Duvan Moncada",
	"license": "ISC",
	"dependencies": {
		"@aws-sdk/client-ses": "^3.693.0",
		"@types/mssql": "^9.1.4",
		"colors": "^1.4.0",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-fileupload": "^1.5.2",
		"express-validator": "^7.0.1",
		"fs-extra": "^11.1.1",
		"handlebars": "^4.7.8",
		"i18n": "^0.15.1",
		"mongoose": "^7.5.0",
		"morgan": "^1.10.0",
		"mssql": "^10.0.2",
		"newrelic": "^12.25.0",
		"nodemon": "^3.1.7",
		"path": "^0.12.7",
		"sequelize": "^6.33.0",
		"tedious": "^16.4.0",
		"ts-node": "^10.9.2",
		"tsc-alias": "^1.8.10",
		"tsconfig-paths": "^4.2.0",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@types/cors": "^2.8.14",
		"@types/express": "^4.17.17",
		"@types/express-fileupload": "^1.4.1",
		"@types/i18n": "^0.13.6",
		"@types/morgan": "^1.9.5",
		"@types/sequelize": "^4.28.15",
		"copyfiles": "^2.4.1",
		"ts-node-dev": "^2.0.0",
		"tslint": "^6.1.3",
		"typescript": "^5.2.2"
	}
}
